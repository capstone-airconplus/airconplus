            android:textColor="#FFFFFF"
            android:textSize="20dp" />
        <TextView
            android:id="@+id/winiacrawling2"
            android:gravity="center"
            android:layout_width="380dp"
            android:layout_height="60dp"
            android:layout_marginBottom="10dp"
            android:paddingLeft="8dp"
            android:paddingRight="8dp"
            android:text="에어컨 가까이는 시원한데 실내온도는 내려가지 않아요"
            android:background="@drawable/button_2"
            android:textColor="#FFFFFF"
            android:textSize="20dp" />
        <TextView
            android:id="@+id/winiacrawling3"
            android:gravity="center"
            android:layout_width="380dp"
            android:layout_height="60dp"
            android:layout_marginBottom="10dp"
            android:paddingLeft="8dp"
            android:paddingRight="8dp"
            android:text="실외기가 가동되지 않습니다"
            android:background="@drawable/button_2"
            android:textColor="#FFFFFF"
            android:textSize="20dp" />
        <TextView
            android:id="@+id/winiacrawling4"
            android:gravity="center"
            android:layout_width="380dp"
            android:layout_height="60dp"
            android:layout_marginBottom="10dp"
            android:paddingLeft="8dp"
            android:paddingRight="8dp"
            android:text="에어컨 필터청소는 얼마 주기로 해야 하나요?"
            android:background="@drawable/button_2"
            android:textColor="#FFFFFF"
            android:textSize="20dp" />
        <TextView
            android:id="@+id/winiacrawling5"
            android:gravity="center"
            android:layout_width="380dp"
            android:layout_height="60dp"
            android:layout_marginBottom="10dp"
            android:paddingLeft="8dp"
            android:paddingRight="8dp"
            android:text="에어컨 청소(냄새제거)는 어떻게 하나요?"
